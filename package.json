{
  "name": "managementconsole",
  "version": "3.1.0",
  "description": "managementconsole",
  "scripts": {
    "generate": "generate",
    "generate:component": "npm run generate component",
    "generate:container": "npm run generate container",
    "generate:page": "npm run generate page",
    "start": "./node_modules/.bin/cross-env NODE_ENV=hmr webpack-dev-server --config webpack.config.js --inline --hot --history-api-fallback --progress",
    "start_win": "set NODE_ENV=hmr && webpack-dev-server --config webpack.config.js --inline --hot --history-api-fallback --progress",
    "build": "./node_modules/.bin/eslint ./src && ./node_modules/.bin/rimraf dist && npm run build:dll && webpack --config webpack.config.js --sort-assets-by --progress",
    "test": "NODE_ENV=test jest --coverage -u",
    "test-watch": "NODE_ENV=test jest --watch -- -u",
    "build:dll": "webpack --config webpack.dll.js --progress",
    "eslint": "./node_modules/.bin/eslint ./src",
    "eslint:fix": "./node_modules/.bin/eslint ./src --fix"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "coveragePathIgnorePatterns": [
      "index.js",
      "<rootDir>/src/config"
    ],
    "browser": true,
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleNameMapper": {
      "^.*\\.(css|scss)$": "<rootDir>/src/config/test-config.js"
    },
    "setupFiles": [
      "<rootDir>/src/config/global-mocks.js"
    ]
  },
  "dependencies": {
    "axios": "^0.12.0",
    "babel-polyfill": "^6.3.14",
    "base-64": "0.1.0",
    "browser-filesaver": "1.1.1",
    "chart.js": "^1.1.1",
    "draft-js": "^0.10.0",
    "draftjs-to-html": "^0.7.0",
    "eslint-loader": "^1.6.3",
    "google-map-react": "^0.20.x",
    "jquery": "2.2.4",
    "json2csv": "3.4.2",
    "lodash": "4.13.1",
    "moment": "^2.13.0",
    "moment-timezone": "0.5.9",
    "q": "1.4.1",
    "react": "~15.4",
    "react-addons-css-transition-group": "15.4.2",
    "react-bootstrap": "0.30.7",
    "react-bootstrap-datetimepicker": "0.0.22",
    "react-css-modules": "^3.7.10",
    "react-datepicker": "^0.27.0",
    "react-datetime": "^2.5.0",
    "react-dom": "~15.4",
    "react-draft-wysiwyg": "^1.7.7",
    "react-google-maps": "4.10.3",
    "react-loader": "^2.4.0",
    "react-motion": "^0.4.2",
    "react-notification-system": "^0.2.12",
    "react-rater": "^0.3.5",
    "react-redux": "^4.2.1",
    "react-router": "^2.4.1",
    "react-select": "^1.0.0-beta14",
    "react-tween-state": "^0.1.4",
    "recharts": "^0.13.3",
    "redux": "^3.2.1",
    "redux-action": "1.1.0",
    "redux-form": "^5.3.2",
    "redux-logger": "^2.4.0",
    "redux-mock-store": "1.2.2",
    "redux-thunk": "^1.0.3",
    "rrmb-generator-utils": "^1.0.1",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.7.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "6.16.0",
    "cross-env": "^3.2.3",
    "css-loader": "0.26.2",
    "enzyme": "2.6.0",
    "enzyme-to-json": "1.3.0",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "expect": "^1.6.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "2.24.1",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "15.4",
    "react-hot-loader": "1.3.1",
    "react-transform-hmr": "1.0.4",
    "resolve-url": "^0.2.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "2.6.1",
    "rrmb-generator": "^2.1.13",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.2.0"
  }
}
